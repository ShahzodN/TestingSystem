@model List<Course>

<div style="display: flex; justify-content: center;"><h1>Доступные курсы</h1></div>
<ul style="padding: 20px">
	@for (int i = 0; i < Model.Count; i++)
	{
		<li><a asp-action="SelectChapter" asp-controller="Home" asp-route-courseId="@Model[i].Id">@Model[i].FullName</a></li>
	}
</ul>